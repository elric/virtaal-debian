			state			= PRELIGHT
			file			= "Combo/combo-prelight.png"
			border			= { 22, 22, 5, 5}
			stretch			= TRUE
	    }
	    
	    # Combobox
	    image
	    {
		    function		= BOX
		    recolorable		= TRUE
		    state			= NORMAL
		    file			= "Combo/combo-normal.png"
		    border			= { 22, 22, 5, 5}
		    stretch			= TRUE
	    }
		
		# Combobox (clicked)
	  	image
	    {
		    function		= BOX
		    recolorable		= TRUE
		    state			= ACTIVE
		    file			= "Combo/combo-prelight.png"
		    border			= { 22, 22, 5, 5}
		    stretch			= TRUE
	    }
	    
