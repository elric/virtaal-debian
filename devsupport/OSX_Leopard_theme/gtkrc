	 	image
	    {
		    function		= BOX
		    recolorable		= TRUE
		    state			= INSENSITIVE
		    file			= "Combo/combo-normal.png"
		    border			= { 22, 22, 5, 5}
		    stretch			= TRUE
	    }
	    
