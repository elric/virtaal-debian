	    {
		    function		= TAB
		    state			= INSENSITIVE
		    recolorable		= TRUE
		    overlay_file	= "Combo/combo-arrow-insens.png"
		    overlay_stretch	= FALSE
	    }
	    
	    # Combobox arrow 
	    image
	    {
		    function		= TAB
		    recolorable		= TRUE
		    state			= NORMAL
		    overlay_file	= "Combo/combo-arrow.png"
		    overlay_border	= { 0, 0, 0, 0 }
		    overlay_stretch	= FALSE
	    }
	    
	    # Combobox arrow (mouse over)
	  	image
	    {
		    function		= TAB
		    recolorable		= TRUE
		    state			= PRELIGHT
		    overlay_file	= "Combo/combo-arrow-prelight.png"
		    overlay_border	= { 0, 0, 0, 0 }
		    overlay_stretch	= FALSE
	    }
	}
}

