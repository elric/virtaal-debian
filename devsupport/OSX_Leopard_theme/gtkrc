		    overlay_stretch		= FALSE
		    orientation			= VERTICAL
		}
		image
		{
		    function			= HANDLE
		    overlay_file		= "Handles/handle-h.png"
		    overlay_stretch 	= FALSE
		    orientation			= HORIZONTAL
		}
	}
}


style "flat" = "default"
{
	engine "pixmap"
	{
		image
		{
		    function = SHADOW
		}
	}
}

style "layout"	= "default"
{
	engine "pixmap"
	{
		image
		{
		    function		= SHADOW
		    detail			= "entry"
		    shadow			= IN
		    recolorable		= FALSE
		    file			= "Shadows/text.png"
		    border			= { 1, 1, 1, 1 }
		    stretch			= TRUE
		}
		image
		{
		    function		= BOX
		    detail			= "button"
		    state			= NORMAL
		    file			= "Buttons/button-normal.png"
		    recolorable		= TRUE
		    border			= { 2, 3, 2, 3 }
		    stretch			= TRUE
		}
	}
}


style "gtktreeview" 
{
  base[NORMAL]     	= "#ffffff"
  base[ACTIVE]     	= "#007ed6"#d4d4d4"0060ca
  base[PRELIGHT]   	= "#FFFFFF"
  base[SELECTED]	= "#007ed6" #b2b2b2"0060ca
  text[ACTIVE]		= "#FFFFFF"
	GtkTreeView::odd_row_color = "#ffffff"  #e4ebf5
	GtkTreeView::even_row_color = "#D5DCE4"
    engine "clearlooks" 
    	{
		colorize_scrollbar = TRUE
		menubarstyle      = 2      # 0 = flat, 1 = sunken, 2 = flat gradient
		toolbarstyle      = 1      # 0 = flat, 1 = enable effects
		animation         = FALSE
		style             = GUMMY
    	}
   
}
class "GtkTreeView" style "gtktreeview"

