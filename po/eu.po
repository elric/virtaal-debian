#: ../bin/virtaal:40
msgid "You must specify a directory or a translation store file for --terminology"
msgstr ""
"Direktorio bat edo itzulpen \"denda\" bat ezarri behar duzu --terminology "
"erabiltzeko"

#: ../bin/virtaal:44
#, c-format
msgid "%prog [options] [translation_file]"
msgstr "%prog [aukerak] [itzulpen_fitxategia]"

#: ../bin/virtaal:47
msgid "PROFILE"
msgstr "PROFILA"

#: ../bin/virtaal:48
msgid "Perform profiling, storing the result to the supplied filename."
msgstr "Profilak erabili, emaitza emandako fitxategian gordetzen."

#: ../bin/virtaal:50
msgid "LOG"
msgstr "ERREGISTROA"

#: ../bin/virtaal:51
msgid "Turn on logging, storing the result to the supplied filename."
msgstr "Erregistroa gaitu emaitzak emandako fitxategian gordeaz."

#: ../bin/virtaal:53
msgid "CONFIG"
msgstr "KONFIG"

#: ../bin/virtaal:54
msgid "Use the configuration file given by the supplied filename."
msgstr "Emandako fitxategiko konfigurazioa erabili."

#: ../bin/virtaal:55
msgid "TERMINOLOGY"
msgstr "TERMINOLOGIA"

#: ../bin/virtaal:58
msgid "Specify a directory containing terminology files"
msgstr "Ezarri terminologia fitxategiak dituen direktorioa"

#: ../bin/virtaal:76
msgid "Could not open log file '%(filename)s'"
msgstr "Ezin da '%(filename)s'erregistro-fitxategia ireki"

#: ../bin/virtaal:83
msgid "Could not read configuration file '%(filename)s'"
msgstr "Ezin da '%(filename)s' fitxategitik konfigurazioa irakurri."

#: ../bin/virtaal:87
msgid "invalid number of arguments"
msgstr "argumentu kopuru baliogabea"

#: ../bin/virtaal:112
msgid "Could not open profile file '%(filename)s'"
msgstr "Ezin da '%(filename)s' profil fitxategia ireki"

